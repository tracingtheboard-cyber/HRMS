{
    "name": "cactus-group/payslip-generator",
    "description": "CACTUS GROUP (PTE. LTD.) Payslip Generator using mPDF",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "mpdf/mpdf": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

